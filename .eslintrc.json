{
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [ "warn", "tab", { "SwitchCase": 1 } ],
      "no-console": 1,
      "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
      "semi": [ "warn", "never" ]
    }
  }